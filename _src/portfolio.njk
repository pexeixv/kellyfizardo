---
title: Portfolio
layout: 'base.njk'
---

{% include 'header.njk' %}

<section class="gallery_outer outer">
    <div class="gallery gallery_inner inner">
        <h2 class="section_title gallery_title">
            Portfolio
        </h2>
        <div class="gallery_filters">
            <button onclick="filterGallery('all',this)" class="gallery_filter gallery_filter-active">All</button>
            <button onclick="filterGallery('product',this)" class="gallery_filter gallery_filter-product">Products</button>
            <button onclick="filterGallery('food',this)" class="gallery_filter gallery_filter-food">Food</button>
            <button onclick="filterGallery('architecture',this)" class="gallery_filter gallery_filter-architecture">Architecture</button>
        </div>
        <div class="gallery_grid">

            <div class="gallery_column gallery_column--1"></div>
            <div class="gallery_column gallery_column--2"></div>
            <div class="gallery_column gallery_column--3"></div>
            <div class="gallery_column gallery_column--4"></div>
            {% for image in gallery %}
                <img loading="lazy" src="//ik.imagekit.io/gavin/kellyfizardo/{{image.category}}/tr:w-450,pr-true,q-70/{{image.slug}}" class="gallery_photo gallery_photo-{{image.category}}" data-slug="{{image.slug}}" data-category="{{image.category}}">
            {% endfor %}

        </div>
    </div>
</section>



{% include 'footer.njk' %}



<script src="/js/colcade.js"></script>
<script src="/js/gallery.js"></script>
<script src="/js/lightbox.js"></script>